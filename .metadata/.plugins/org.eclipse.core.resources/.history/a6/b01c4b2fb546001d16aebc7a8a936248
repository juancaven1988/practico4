package grafica.controladores;

import javax.swing.JFrame;

import grafica.ventana.DuenosFrame;
import grafica.ventana.MascotasFrame;

public class MenuController {

	
	private DuenosFrame duenos;
	private MascotasFrame mascotas;
	
	public void insertarDueno(DuenosFrame dueno) {
		
		if(duenos == null)
		{
			duenos = new DuenosFrame();
		}
		
		duenos.activarInsertar();
	}
	
	
}
