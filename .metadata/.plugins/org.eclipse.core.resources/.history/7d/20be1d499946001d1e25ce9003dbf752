package grafica.controladores;

import javax.swing.JFrame;

import grafica.ventana.DuenosFrame;
import grafica.ventana.MainWindow;
import grafica.ventana.MascotasFrame;

public class mainWindowController  {
	
	DuenosFrame duenos;
	MascotasFrame mascotas;
	MainWindow mainWindow;


	public mainWindowController()
	{
		
	}
	
	public void openPanelDuenos(JFrame mainFrame) {
		
		mainFrame.setVisible(false);
		duenos = new DuenosFrame();
		
	}

	public void openPanelMascotas(JFrame mainFrame) {
		
		mainFrame.setVisible(false);
		mascotas = new MascotasFrame();
		
	}
	

	
}
